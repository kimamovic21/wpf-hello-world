<Window x:Class="WPFHelloWorld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFHelloWorld"
        mc:Ignorable="d"
        Title="Hello World Window" Height="400" Width="500">
    <StackPanel>
        <DatePicker
            SelectedDate="03/05/2025"   
            SelectedDateFormat="Long"
            SelectedDateChanged="DatePicker_SelectedDateChange"
        >
            <DatePicker.BlackoutDates>
                <CalendarDateRange Start="03/01/2025" End="03/04/2025">
                    
                </CalendarDateRange>
            </DatePicker.BlackoutDates>
        </DatePicker>
    </StackPanel>
</Window>